AACtB,UAAG,CAAC,KAAK,CAAC,IAAV,EAAe;AACb,eAAO,EAAP;AACF;;AACA,aAAO,KAAK,CAAC,QAAN,CAAe,MAAf,CAAsB,UAAC,OAAD;AAAA,eACzB,OAAO,CAAC,IAAR,CAAa,QAAb,CAAsB,KAAK,CAAC,IAA5B,CADyB;AAAA,OAAtB,CAAP;AAGF;;AAEA,2CAAY,MAAM,CAAC,KAAD,CAAlB;AAA2B,MAAA,YAAY,EAAZ,YAA3B;AAAyC,MAAA,YAAY,EAAZ,YAAzC;AAAuD,MAAA,aAAY,EAAZ;AAAvD;AACF;AA7Ca,CAAf","sourcesContent":["<template>\n<div class=\"w-full flex justify-center\">\n<input placeholder=\"Enter Pokemon here\" type=\"text\" \n    class=\"mt-10 p-2 border-blue-500 border-2\" v-model=\"text\" />\n\n</div>\n\n  <div class=\"mt-10 p-4 flex flex-wrap justify-center\">\n\n    \n    <div\n      class=\"ml-4 text-2xl text-blue-400\"\n      v-for=\"(pokemon, idx) in filteredPokemon\"\n      :key=\"idx\"\n    >\n      <router-link :to=\"`/about/${getPokemonId(pokemon.name)}`\">\n        {{ pokemon.name }}\n      </router-link>\n    </div>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { reactive, toRefs, computed } from \"vue\";\nexport default {\n  setup() {\n    const state = reactive({\n      pokemons: [],\n      filteredPokemon: computed(()=> updatePokemon()), \n      text: \"\",\n      urlIdLookup: {}\n      \n    });\n\n    const fetchPokemon = () => {\n      axios.get('https://pokeapi.co/api/v2/pokemon?offset=0')\n      .then(response => {\n        state.pokemons = response.data.results\n      })\n    }\n\n    fetchPokemon()\n\n    const getPokemonId = (item) => {\n      return state.pokemons.findIndex((p) => p.name === item);\n    };\n\n    // fetch(\"https://pokeapi.co/api/v2/pokemon?offset=0\")\n    //   .then((res) => res.json())\n    //   .then((data) => {\n    //     console.log(data);\n    //     state.pokemons = data.results;\n    //     state.urlIdLookup = data.results.reduce((acc, cur, idx)=> \n    //        acc = {...acc, [cur.name]:idx+1 }\n    //     ,{})\n    //     console.log('url',state.urlIdLookup+1)\n        \n    //   });\n\n    function updatePokemon(){\n      if(!state.text){\n        return [] \n      }\n      return state.pokemons.filter((pokemon)=> \n          pokemon.name.includes(state.text)\n      )\n    }\n\n    return { ...toRefs(state), fetchPokemon, getPokemonId, updatePokemon };\n  }\n};\n</script>\n"],"sourceRoot":""}]}